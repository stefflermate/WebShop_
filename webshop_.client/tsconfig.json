{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [ "DOM", "DOM.Iterable", "ESNext" ],
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx"
  },
  "include": [ "src" ]
}
